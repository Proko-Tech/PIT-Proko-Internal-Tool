<div class="container">
<div class="row">
    <div class="col s12 m5" id = test>
    <div class="card-panel ">
       <div class='col s8'>
           <div class = titles>
           <div class = 'title'>
               <p> Ticket  Detail</p>
           </div>
           <div class = 'time'>
               <p> <%= spot[0].updated_at%></p>
           </div>
           </div>
           <div id = 'map'></div>
           <script>
               function initMap(){
                   var options = {
                       zoom: 10,
                       center: {lat:<%=spot[0].lat %>,lng:<%=spot[0].long %>}
                   }
                   var map = new google.maps.Map(document.getElementById('map'),options)
               }
           </script>
           <script
                   src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDlortmnrf9V5j7FosAw-p_Bol5kY0lD7g&callback=initMap&libraries=&v=weekly"
                   async
           ></script>
       </div>
        <div class="col s4" id = 'box'>
            <div id="subject">
                        <p>
                            Spot Number
                        </p>
            </div>
            <div id="details">
                    <p>
                        <%=spot[0].spot_name %>
                    </p>
            </div>
            <div class="divider"></div>
            <div id="subject">
                <p>
                    Sensor Serial Code
                </p>
            </div>
            <div id="details">
                <p>
                    <%=spot[0].spot_hash %>
                </p>
            </div>
                <div class="divider"></div>
                <div id="subject">
                    <p>
                        Last Update At
                    </p>
                </div>
            <div id="details">
                    <p>
                        <%=spot[0].updated_at %>
                    </p>
            </div>
            <div class="divider"></div>
            <div id="subject">
                <p>
                    Address
                </p>
            </div>
            <div id="details">
                <p>
                    <%=spot[0].address %>
                </p>
            </div>
        </div>
     </div>
</div>
</div>
