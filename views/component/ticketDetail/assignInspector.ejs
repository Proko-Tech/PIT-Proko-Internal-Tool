<!-- Parking lot info card -->
<div class="row">
    <div class="col s12  ">
        <div class="card">
            <div class="card-content">
                <form class="container" action="/ticket_detail" method="POST">
                    <div class="row">
                        <div class="col s8 pull-s2" ><h6>Assign Inspector</h6></div>
                        <div class="col s8 push-s12"><a>Assign</a></div>
                        <div class="col s8 pull-s2"><p>Time</p></div>
                    </div>
                    <div class="row">
                        <div class="input-field col s6">
                            <input id="first-name" placeholder="First Name" type="text" class="Inspector">
                        </div>
                        <div class="input-field col s6">
                            <input id="last-name" placeholder="Last Name" type="text" class="Inspector">
                        </div>
                        <div class="input-field col s12">
                            <input id="email" placeholder="Email" type="text" class="Inspector">
                        </div>
                        <div class="input-field col s12">
                            <input id="message" placeholder="Message" type="text" class="Inspector">
                        </div>
                        </div>

                </form>
            </div>
        </div>
    </div>
</div>

