<% if (capturedImageData.length > 0) { %>
    <div class="row">
    <div class="col s12 m6">
        <div class="card">
            <div class="card-image">
                <img src="<%=capturedImageData[0].image_url%>">
            </div>
            <div class="card-content">
                <p>Picture taken at <%=capturedImageData[0].created_at%></p>
            </div>
        </div>
    </div>
</div>
<% } else {%>
    <h1> Invalid ID </h1>
<% } %>

